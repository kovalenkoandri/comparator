{
  "expo": {
    "name": "comparator",
    "slug": "comparator",
    "privacy": "public",
    "platforms": ["android"],
    "orientation": "default",
    "githubUrl": "https://github.com/kovalenkoandri/price-comparator-front",
    "icon": "./assets/icon.png",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#353542"
    },
    "assetBundlePatterns": ["**/*"],
    "android": {
      "package": "com.kovalenkoandrii.comparator",
      "versionCode": 1
    },
    "description": "Price comparator",
    "owner": "kovalenkoandrii",
    "runtimeVersion": {
      "policy": "appVersion"
    },
    "plugins": [
      [
        "expo-screen-orientation",
        {
          "initialOrientation": "DEFAULT"
        }
      ]
    ]
  }
}
